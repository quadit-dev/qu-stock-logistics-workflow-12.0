<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <record id="view_valued_picking_purchase_order_form" model="ir.ui.view">
        <field name="name">view.valued.picking.purchase.order.form</field>
        <field name="model">purchase.order</field>
        <field name="inherit_id" ref="purchase.purchase_order_form"/>
        <field name="type">form</field>
        <field name="arch" type="xml">
            <xpath expr="//field[@name='incoterm_id']" position="after">
                <field name="valued_picking"/>
            </xpath>
        </field>
    </record>
</odoo>
